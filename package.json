{
	"name": "ssf",
	"version": "1.0.0",
	"scripts": {
		"dev": "blitz dev",
		"build": "blitz build",
		"start": "blitz start",
		"studio": "blitz prisma studio",
		"lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
		"test": "jest",
		"test:watch": "jest --watch",
		"analyze": "cross-env ANALYZE=true next build",
		"ts-lint": "tsc --noEmit --incremental",
		"prepare": "$CI || husky install"
	},
	"prisma": {
		"schema": "db/schema.prisma"
	},
	"prettier": {
		"semi": false,
		"printWidth": 100,
		"tabWidth": 4,
		"singleQuote": true,
		"tabs": true
	},
	"lint-staged": {
		"*.{js,ts,tsx}": [
			"eslint --fix"
		]
	},
	"dependencies": {
		"@mantine/core": "4.2.5",
		"@mantine/dates": "4.2.5",
		"@mantine/dropzone": "4.2.5",
		"@mantine/hooks": "4.2.5",
		"@mantine/next": "4.2.5",
		"@mantine/notifications": "4.2.5",
		"@mantine/prism": "4.2.5",
		"@mantine/rte": "4.2.5",
		"@mantine/ssr": "4.2.5",
		"@prisma/client": "3.15.2",
		"@types/nodemailer": "6.4.4",
		"autoprefixer": "^10.4.8",
		"blitz": "^0.44.3",
		"cors": "^2.8.5",
		"dayjs": "^1.10.7",
		"nanoid": "^3.1.30",
		"nodemailer": "6.7.5",
		"postcss": "^8.4.4",
		"ramda": "0.28.0",
		"react": "^18.2.0",
		"react-countdown": "2.3.2",
		"react-country-flag": "^3.0.2",
		"react-dom": "^18.2.0",
		"react-final-form": "^6.5.7",
		"react-icons": "^4.3.1",
		"slug": "^5.1.1",
		"slugify": "1.6.5",
		"tailwindcss": "3.0.24",
		"zod": "^3.11.6"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "12.1.6",
		"@types/cors": "^2.8.12",
		"@types/multer": "^1.4.7",
		"@types/preview-email": "^2.0.1",
		"@types/ramda": "0.28.11",
		"@types/react": "^18.0.15",
		"@types/slug": "^5.0.3",
		"cross-env": "7.0.3",
		"eslint": "^7.32.0",
		"husky": "^7.0.4",
		"lint-staged": "^11.3.0-beta.2",
		"prettier": "^2.5.1",
		"prettier-plugin-prisma": "^3.6.0",
		"pretty-quick": "^3.1.2",
		"preview-email": "^3.0.5",
		"prisma": "3.15.2",
		"typescript": "~4.5"
	},
	"private": true
